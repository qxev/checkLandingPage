.darwin-css-button {
	background: transparent url(/images/buttons/buttonright1.png) no-repeat
		scroll right top;
	float: left;
	margin: 5px 5px 0 0;
	text-align: center;
}

.darwin-css-button a {
	background: transparent url(/images/buttons/buttonleft1.png) no-repeat
		scroll left top;
	color: #0066CC;
	display: block;
	font-size: 12px;
	line-height: 16px;
	margin: 0 5px 0 0;
	outline-color: -moz-use-text-color;
	outline-style: none;
	outline-width: medium;
	padding: 5px 5px 5px 10px;
	text-decoration: none;
}

.darwin-css-button:hover {
	background: transparent url(/images/buttons/buttonright2.png) no-repeat
		scroll right top;
}

.darwin-css-button a:hover {
	color: #0a0;
	background: transparent url(/images/buttons/buttonleft2.png) no-repeat
		scroll left top;
}

.darwin-float-right {
