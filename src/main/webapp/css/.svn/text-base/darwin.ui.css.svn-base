#darwin-head {
	height: 50px;
	background: #fff;
}

#darwin-head-user {
	margin: 10px 40px 0;
	float: right;
}

#darwin-head-logo {
	margin: 10px 0 0 10px;
	float: left;
	height: 11px;
	width: 120px;
	background: url(/images/logo_sv3.png) scroll no-repeat 0 0;
}

#darwin-navigation {
	height: 30px;
	margin: 0 0 5px 0;
	background: #fff;
	border-bottom: 2px solid green;
}

#darwin-menu {
	margin: 0 10px;
	padding: 0;
	height: 100%;
	list-style: none;
}

#darwin-menu .darwin-menu-list {
	float: left;
	width: 100px;
	height: 15px;
	border: 0 solid #ddd;
	border-left-width: 2px;
	border-top-width: 3px;
	float: left;
	padding: 6px 10px;
	text-align: center;
}

#darwin-menu .darwin-menu-list a {
	text-decoration: none;
}

#darwin-menu .darwin-menu-last-list {
	border-right-width: 2px;
}

#darwin-menu .darwin-menu-selected {
	border-top: 3px solid #389C39;
	color: #389C39;
	font-weight: 600;
}

#darwin-menu .darwin-menu-list a:link,#darwin-menu .darwin-menu-list a:visited,#darwin-menu .darwin-menu-list a:hover
	{
	color: #333333;
}

#darwin-menu .darwin-menu-list a:hover {
	text-decoration: underline;
	color: #389c39;
}

#darwin-menu .darwin-menu-selected a:link,#darwin-menu .darwin-menu-selected a:visited,#darwin-menu .darwin-menu-selected a:hover
	{
	color: #389c39;
}

#darwin-menu .darwin-menu-selected a:hover {
	text-decoration: none;
	color: #389c39;
}

#darwin-footer {
	color: #666;
	background: #fff;
	height: 40px;
	text-align: center;
}

#darwin-footer-copy {
	margin: 10px 0;
	float: left;
	width: 100%;
	text-align: center;
}

#darwin-container {
	background: #eee;
	margin: 0 0 10px 10px;
}

.darwin-text-right {
	text-align: right;
}

