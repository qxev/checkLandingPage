.darwin-no-drag {
	height: 100%;
	width: 11px;
	background: #ddd;
}

.darwin-drag {
	height: 100%;
	width: 11px;
	background: #ddd url(/images/move1.png) no-repeat scroll 0 50%;
	cursor: move;
}

.darwin-dragging {
	position: absolute;
	width: 120px;
	height: 30px;
	color: #fff;
	background: #000;
	padding: 0 0 0 20px;
	z-index: 2;
}

